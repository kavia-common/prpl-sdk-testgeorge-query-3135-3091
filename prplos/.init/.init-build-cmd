mkdir -p "$WS/.logs"
LOG="$WS/.logs/prplos_http_server.log"
# Run npm build; on failure show recent log and exit 3
npm run build --silent || { echo "build failed" >&2; tail -n 200 "$LOG" 2>/dev/null || true; exit 3; }
" }
